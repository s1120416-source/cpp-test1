#include <iostream>
using namespace std;

int main() {
    // 第一部分：用兩層 for 迴圈顯示模式
    for (int i = 1; i <= 6; i++) {
        for (int j = 1; j <= i; j++) {
            cout << "*";
        }
        cout << endl;  // 換行
    }

    // 第二部分：一些數值計算，可能是加法
    int sum = 0;
    for (int i = 1; i <= 5; i++) {
        sum += i;  // 累加數字
    }
    
    cout << "Sum of numbers from 1 to 5 is: " << sum << endl;

    return 0;
}
